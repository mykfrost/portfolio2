--color-accent: #ff3400;\r\n  --color-heading: #1b0760;\r\n  --color-body: #918ca4;\r\n  --color-body-darker: #5c5577;\r\n  --color-border: #ccc;\r\n  --border-radius: 30px;\r\n}\r\n*,\r\n*::after,\r\n*::before {\r\n  box-sizing: border-box;\r\n}\r\n/*Topography*/\r\n\r\n::selection {\r\n  background: var(--color-primary);\r\n  color: #fff;\r\n}\r\n\r\nhtml {\r\n  /*62.5% of 16px */\r\n  font-size: 62.5%;\r\n}\r\n\r\nbody {\r\n  font-family: inter, Arial, Helvetica, sans-serif;\r\n  color: var(--color-body);\r\n  font-size: 2.4rem;\r\n  line-height: 1.5;\r\n  margin: 0.5rem;\r\n}\r\n\r\nh1,\r\nh2,\r\nh3 {\r\n  color: var(--color-heading);\r\n  margin-bottom: 1em;\r\n}\r\n\r\nh1 {\r\n  font-size: 5rem;\r\n}\r\n\r\nh2 {\r\n  font-size: 3rem;\r\n}\r\n\r\nh3 {\r\n  font-size: 2rem;\r\n  font-weight: 500;\r\n}\r\np {\r\n  margin-top: 0;\r\n}\r\n/*My Header*/\r\n\r\nheader {\r\n  background: #2f3061;\r\n}\r\n/*Grids*/\r\n.grid {\r\n  display: grid;\r\n}\r\n\r\n@media screen and (min-width: 768px) {\r\n  .grid--1x2 {\r\n    grid-template-columns: repeat(2, 1fr);\r\n  }\r\n}\r\n\r\n@media screen and (min-width: 1024px) {\r\n  .grid--1x3 {\r\n    grid-template-columns: repeat(3, 1fr);\r\n  }\r\n}\r\n.container {\r\n  max-width: 1140px;\r\n  margin: 0 auto;\r\n}\r\n\r\n/*Collapsibles*/\r\n.nav {\r\n  /* background: #b47fd9; */\r\n  display: flex;\r\n  justify-content: space-between;\r\n  flex-wrap: wrap;\r\n  padding: 0 1rem;\r\n}\r\n\r\n.nav__list {\r\n  width: 100%;\r\n  margin: 0;\r\n}\r\n.nav__item {\r\n  padding: 0.5rem 2rem;\r\n  border-bottom: 1px solid #222;\r\n}\r\n\r\n.nav__item > a {\r\n  color: #d2d0db;\r\n  transition: color 0.3s;\r\n}\r\n\r\n.nav__item > a:hover {\r\n  color: #fff;\r\n}\r\n\r\n.nav__toggler {\r\n  opacity: 0.5;\r\n  transition: box-shadow 0.15s;\r\n  cursor: pointer;\r\n}\r\n\r\n.nav.collapsible--expanded .nav__toggler {\r\n  opacity: 1;\r\n  box-shadow: 0 0 0 3px #666;\r\n  border-radius: 5px;\r\n}\r\n\r\n.nav__brand {\r\n  transform: translateY(5px);\r\n}\r\n\r\n@media screen and (min-width: 768px) {\r\n  .nav__toggler {\r\n    display: none;\r\n  }\r\n  .nav__list {\r\n    width: auto;\r\n    display: flex;\r\n    font-size: 1.6rem;\r\n    max-height: 100%;\r\n    opacity: 1;\r\n  }\r\n\r\n  .nav__item {\r\n    border: 0;\r\n  }\r\n}\r\n\r\n/*Lists*/\r\n\r\n.list {\r\n  list-style: none;\r\n  padding-left: 0;\r\n}\r\n\r\n.list--inline .list__item {\r\n  display: inline-block;\r\n  margin-right: 2rem;\r\n}\r\n\r\n@media screen and (min-width: 1024px) {\r\n  .list--tick .list__item {\r\n    padding-left: 0;\r\n  }\r\n}\r\n\r\n/*icons*/\r\n.icon {\r\n  width: 40px;\r\n  height: 40px;\r\n}\r\n\r\n.icon--small {\r\n  width: 30px;\r\n  height: 30px;\r\n}\r\n\r\n.icon--primary {\r\n  fill: var(--color-primary);\r\n}\r\n\r\n.icon--white {\r\n  fill: white;\r\n}\r\n/*icons*/\r\n.icon {\r\n  width: 40px;\r\n  height: 40px;\r\n}\r\n\r\n.icon--small {\r\n  width: 30px;\r\n  height: 30px;\r\n}\r\n\r\n.icon--primary {\r\n  fill: var(--color-primary);\r\n}\r\n\r\n.icon--white {\r\n  fill: rgb(107, 96, 233);\r\n}\r\n\r\n.icon-container {\r\n  background: #f3f9fa;\r\n  width: 64px;\r\n  border-radius: 100%;\r\n  display: inline-flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n.icon-container--accent {\r\n  background: var(--color-accent);\r\n}\r\n\r\n/*Projects*/\r\n.project {\r\n  background: #f7fff7;\r\n}\r\n/*Work experience*/\r\n.work-experience {\r\n  background: #a1c6d7;\r\n}\r\n/*Education*/\r\n/*Footer*/\r\n.footer {\r\n  background: #343434;\r\n}\r\n"},"lineCount":224}},"error":null,"hash":"98dcd352d77480c311579ef8adcf2bf6","cacheData":{"env":{}}}